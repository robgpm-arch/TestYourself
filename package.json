{
  "name": "youware-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "cap:sync": "npx cap sync",
    "cap:android": "npm run build && npx cap sync android && npx cap open android",
    "cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
    "android:dev": "npx cap run android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "icons:generate": "npx @capacitor/assets generate",
    "firebase:deploy": "npm run build && firebase deploy",
    "functions:dev": "cd functions && npm run serve",
    "functions:deploy": "cd functions && npm run deploy",
    "seed:content": "node scripts/import-content.mjs",
    "scan:project": "node scripts/scan-project.mjs",
    "seed:dry": "node scripts/import-firestore.mjs --dry",
    "seed:firestore": "node scripts/import-firestore.mjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/local-notifications": "^7.0.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "ai": "^4.3.16",
    "cannon-es": "^0.20.0",
    "clsx": "^2.1.0",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^11.0.8",
    "gsap": "^3.13.0",
    "i18next": "^23.10.1",
    "i18next-browser-languagedetector": "^7.2.0",
    "lucide-react": "^0.533.0",
    "matter-js": "^0.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.30.1",
    "react-use": "^17.6.0",
    "three": "^0.179.1",
    "zod": "^3.25.67",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@types/matter-js": "^0.20.0",
    "@types/node": "^24.8.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@youware/vite-plugin-react": "^1.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fast-glob": "^3.3.3",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "~5.8.3",
    "vite": "^7.1.10"
  }
}
