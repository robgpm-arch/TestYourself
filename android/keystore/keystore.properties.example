# Keystore configuration for release builds
# Rename this file to 'keystore.properties' and fill in your actual values
# DO NOT commit the actual keystore.properties file to version control

storePassword=YOUR_STORE_PASSWORD
keyPassword=YOUR_KEY_PASSWORD  
keyAlias=YOUR_KEY_ALIAS
storeFile=YOUR_KEYSTORE_FILE_PATH

